{
  "manifest_version": 3,
  "name": "Youtubeキーワードミュート",
  "version": "0.0.1",
  "description": "特定のキーワードを含むYoutubeのビデオを非表示にする",
  "icons": {
    "128": "img/128.png",
    "48": "img/48.png",
    "16": "img/16.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["main.js"]
    }
  ],
  "options_page": "options.html"
}